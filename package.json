{
  "name": "argocd-diff-action",
  "version": "0.0.0",
  "private": true,
  "description": "ArgoCD Diff GitHub Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quizlet/argocd-diff-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "argocd"
  ],
  "author": "Quizlet",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.0",
    "@actions/tool-cache": "^1.7.1",
    "@types/node-fetch": "^2.5.10",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^29.5.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
